    <classes>
      <class name="CrossBrowser.Signin"/>
      <class name="CrossBrowser.Createaccount"/>
      <class name="CrossBrowser.Assertion"/>
    </classes>
     </test> <!-- Test -->
</suite> <!-- Suite -->
